{
	"parser": "babel-eslint",
	"rules": {
	    "semi": 0,
	    "key-spacing": [2, {
		    "singleLine": {
		        "beforeColon": false,
		        "afterColon": true
		    },
		    "multiLine": {
		        "beforeColon": true,
		        "afterColon": true,
		        "align": "colon"
		    }
		}]
	},
	"extends": "airbnb",
}
